LD 0 0 ; orig dir

LDC 0
CEQ
TSEL $RTN_RIGHT $NOT_UP
    LDC 1 ; $RTN_RIGHT
    RTN

    LD 0 0 ; $NOT_UP
    LDC 2
    CEQ
    TSEL $RTN_LEFT $NOT_DOWN
        LDC 3 ; $RTN_LEFT
        RTN

        LD 0 0 ; $NOT_DOWN
        LDC 1
        CEQ
        TSEL $RTN_UP $RTN_DOWN
            LDC 0 ; $RTN_UP
            RTN

            LDC 2 ; $RTN_DOWN
            RTN
